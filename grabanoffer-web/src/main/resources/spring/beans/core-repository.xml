<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="repository" class="com.gao.core.framework.utils.RepositoryFactoryBean"
		p:repositoryConf="classpath:jackrabbit/jackrabbit-repository.xml"
		p:repositoryProps="classpath:jackrabbit/repository.properties"
		p:repositoryHome="/media/Drive 1/Softwares/idea-1/codebase/git/customsitecreator/gao_repository"
		p:useRepositoryFactory="true" p:createRepositoryHome="true" />

	<bean id="repositoryDao" class="com.gao.core.image.dao.ImageDaoImpl"
		p:repository-ref="repository" />

	<bean id="repositoryService" class="com.gao.core.image.service.ImageServiceImpl"
		p:repositoryDao-ref="repositoryDao" />

